<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/links"
        android:layout_below="@+id/hm1"
        android:orientation="horizontal">
        <TextView
            android:layout_width="165dp"
            android:layout_height="50dp"
            android:id="@+id/historicallink"
            android:gravity="center"
            android:textStyle="bold"
            android:text=" To view in chart format click "/>
        <TextView
            android:layout_width="105dp"
            android:layout_height="50dp"
            android:id="@+id/currentlinks"
            android:gravity="center"
            android:textColor="#3c8dbc"
            android:textStyle="bold"
            android:text=" Current Year Link "/>
        <TextView
            android:layout_width="105dp"
            android:layout_height="50dp"
            android:id="@+id/yearslink"
            android:gravity="center"
            android:textColor="#3c8dbc"
            android:textStyle="bold"
            android:text=" Historical Link "/>

    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/text34"
        android:layout_below="@+id/links"
        android:text="Current Year:"
        />

    <RelativeLayout
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/text34"
        android:id="@+id/relativeTop1">
        <TextView
            android:layout_width="80dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:text="3 Weeks Ago"
            android:background="@drawable/weather_textview_top_background"
            android:id="@+id/threeweeksago1"/>
        <TextView
            android:layout_width="80dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:text="2 Weeks Ago"
            android:background="@drawable/weather_textview_top_background"
            android:layout_toRightOf="@+id/threeweeksago1"
            android:id="@+id/twoweeksago1"/>
        <TextView
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:text="Previous Week"
            android:background="@drawable/weather_textview_top_background"
            android:layout_toRightOf="@+id/twoweeksago1"
            android:id="@+id/previousweek1"/>
        <TextView
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:text="Current"
            android:background="@drawable/weather_textview_top_background"
            android:layout_toRightOf="@+id/previousweek1"
            android:id="@+id/currentweek1"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relativeTop1"
        android:id="@+id/relativeTop">
        <TextView
            android:layout_width="80dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:id="@+id/threeweeksago"/>
        <TextView
            android:layout_width="80dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:layout_toRightOf="@+id/threeweeksago"
            android:id="@+id/twoweeksago"/>
        <TextView
            android:layout_width="90dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:layout_toRightOf="@+id/twoweeksago"
            android:id="@+id/previousweek"/>
        <TextView
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:layout_toRightOf="@+id/previousweek"
            android:id="@+id/currentweek"/>
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/historical"
        android:layout_below="@+id/relativeTop"
        android:text="Historical:"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/week12"
        android:layout_below="@+id/historical"
        android:background="@drawable/weather_textview_top_background"
        android:text="Week11:"
        />
    <RelativeLayout
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/week12"
        android:id="@+id/relative1">
        <TextView
            android:layout_width="110dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:id="@+id/year1"/>
        <TextView
            android:layout_width="120dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:layout_toRightOf="@+id/year1"
            android:id="@+id/year2"/>
        <TextView
            android:layout_width="130dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:layout_toRightOf="@+id/year2"
            android:id="@+id/year3"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relative1"
        android:id="@+id/relative2">
        <TextView
            android:layout_width="110dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:id="@+id/year11"/>
        <TextView
            android:layout_width="120dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:layout_toRightOf="@+id/year11"
            android:id="@+id/year22"/>
        <TextView
            android:layout_width="130dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:layout_toRightOf="@+id/year22"
            android:id="@+id/year33"/>
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/week10"
        android:layout_below="@+id/relative2"
        android:background="@drawable/weather_textview_top_background"
        android:text="Week10:"
        />
    <RelativeLayout
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/week10"
        android:id="@+id/relative4">
        <TextView
            android:layout_width="110dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:id="@+id/year111"/>
        <TextView
            android:layout_width="120dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:layout_toRightOf="@+id/year111"
            android:id="@+id/year222"/>
        <TextView
            android:layout_width="130dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:layout_toRightOf="@+id/year222"
            android:id="@+id/year333"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relative4"
        android:id="@+id/relative5">
        <TextView
            android:layout_width="110dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:id="@+id/year1111"/>
        <TextView
            android:layout_width="120dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:layout_toRightOf="@+id/year1111"
            android:id="@+id/year2222"/>
        <TextView
            android:layout_width="130dp"
            android:layout_height="50dp"
            android:textSize="15dp"
            android:gravity="center"
            android:background="@drawable/weather_textview_top_background"
            android:layout_toRightOf="@+id/year2222"
            android:id="@+id/year3333"/>
    </RelativeLayout>

</RelativeLayout>